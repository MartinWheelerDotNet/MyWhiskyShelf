{
  "stryker-config": {
    "solution": "MyWhiskyShelf.sln",
    "concurrency": 5,
    "mutate": [
      "!MyWhiskyShelf.Database/Contexts/*.cs",
      "!MyWhiskyShelf.Database/Configurations/*.cs",
      "!MyWhiskyShelf.TestHelpers/*.cs",
      "!**/*.Generated.cs"
    ],
    "ignoreMutations": [
      {
        "mutationName": "OrAssignmentExpression",
        "fileName": "MyWhiskyShelf.Database/Encoders/FlavourProfileEncoder.cs"
      },
      {
        "mutationName": "ShiftRight",
        "fileName": "MyWhiskyShelf.Database/Encoders/FlavourProfileEncoder.cs"
      }
    ]
  }
}